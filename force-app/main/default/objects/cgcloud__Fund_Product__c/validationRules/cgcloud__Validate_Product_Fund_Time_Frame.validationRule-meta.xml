<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Validate_Product_Fund_Time_Frame</fullName>
    <active>true</active>
    <description>Fund Time Frame must be within Product Time Frame (Fund Date From - Fund Date Thru).</description>
    <errorConditionFormula>AND(!$Setup.cgcloud__Validation_Rules_Setting__c.cgcloud__Bypass_Validation__c,
				OR(cgcloud__Fund__r.cgcloud__Valid_From__c &lt; cgcloud__Product__r.cgcloud__Fund_Valid_From__c, 
				cgcloud__Fund__r.cgcloud__Valid_Thru__c &gt; cgcloud__Product__r.cgcloud__Fund_Valid_Thru__c))</errorConditionFormula>
    <errorMessage>Fund Time Frame must be within Product Time Frame (Fund Date From - Fund Date Thru).</errorMessage>
</ValidationRule>

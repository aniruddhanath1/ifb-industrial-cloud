<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PunchOutTime__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  ISBLANK(Punch_Out_Time__c),
  null,
  TEXT(YEAR(DATEVALUE(Punch_Out_Time__c))) &amp; &apos;-&apos; &amp;
  LPAD(TEXT(MONTH(DATEVALUE(Punch_Out_Time__c))), 2, &apos;0&apos;) &amp; &apos;-&apos; &amp;
  LPAD(TEXT(DAY(DATEVALUE(Punch_Out_Time__c))), 2, &apos;0&apos;) &amp; &apos;T&apos; &amp;
  LPAD(TEXT(VALUE(MID(TEXT(Punch_Out_Time__c + (5/24) + (30/1440)), 12, 2))), 2, &apos;0&apos;) &amp; &apos;:&apos; &amp;
  LPAD(TEXT(VALUE(MID(TEXT(Punch_Out_Time__c + (5/24) + (30/1440)), 15, 2))), 2, &apos;0&apos;) &amp; &apos;:&apos; &amp;
  LPAD(TEXT(VALUE(MID(TEXT(Punch_Out_Time__c + (5/24) + (30/1440)), 18, 2))), 2, &apos;0&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Editing_Expires_On</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISCHANGED(SBQQ__ExpirationDate__c),
    NOT(ISBLANK(SBQQ__ExpirationDate__c)),
    !BypassValidationCheckbox__c 
)</errorConditionFormula>
    <errorMessage>You cannot change the &apos;Expires On&apos; date once it is set.</errorMessage>
</ValidationRule>

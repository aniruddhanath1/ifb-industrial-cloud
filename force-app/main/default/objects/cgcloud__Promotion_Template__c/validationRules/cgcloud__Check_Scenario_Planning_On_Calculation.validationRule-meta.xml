<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Check_Scenario_Planning_On_Calculation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(cgcloud__Calculation_Mode__c, &quot;No&quot;), (cgcloud__Enable_Scenario_Planning__c))</errorConditionFormula>
    <errorDisplayField>cgcloud__Calculation_Mode__c</errorDisplayField>
    <errorMessage>Enable Scenario Planning can&apos;t be checked if the Calculation Mode is &apos;No&apos;.</errorMessage>
</ValidationRule>

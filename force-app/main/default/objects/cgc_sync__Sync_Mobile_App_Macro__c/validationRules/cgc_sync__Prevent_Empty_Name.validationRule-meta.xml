<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgc_sync__Prevent_Empty_Name</fullName>
    <active>true</active>
    <description>Do not allow creation of macro without name.</description>
    <errorConditionFormula>ISBLANK(Name)</errorConditionFormula>
    <errorMessage>Macro name must be defined.</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Sales_Org_Check</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT(cgcloud__Inventory_Transaction_Template__r.cgcloud__Sales_Org__c)  &lt;&gt;  TEXT(cgcloud__Inventory__r.cgcloud__Sales_Org__c) &amp;&amp;
        ( NOT( ISBLANK(TEXT(cgcloud__Inventory_Transaction_Template__r.cgcloud__Sales_Org__c))) &amp;&amp; NOT( ISBLANK(TEXT(cgcloud__Inventory__r.cgcloud__Sales_Org__c) )))</errorConditionFormula>
    <errorMessage>The Inventory Transaction Template and the Inventory must have the same Sales Org.</errorMessage>
</ValidationRule>

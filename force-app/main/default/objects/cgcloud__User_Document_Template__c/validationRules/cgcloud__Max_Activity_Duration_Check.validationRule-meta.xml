<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Max_Activity_Duration_Check</fullName>
    <active>true</active>
    <errorConditionFormula>OR(cgcloud__Max_Activity_Duration__c &lt; 0, cgcloud__Max_Activity_Duration__c &gt; 24)</errorConditionFormula>
    <errorMessage>Max Activity must be a number between 0 and 24.</errorMessage>
</ValidationRule>

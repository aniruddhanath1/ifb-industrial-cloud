<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Multiple_Time_Level_Cant_Be_Disabled</fullName>
    <active>true</active>
    <description>Prevents disabling KPI time intervals for months and quarters.</description>
    <errorConditionFormula>AND(
ISCHANGED(cgcloud__Edits_On_Multiple_Time_Level__c),
ISPICKVAL(PRIORVALUE( cgcloud__Edits_On_Multiple_Time_Level__c ), &quot;Enabled&quot;)
)</errorConditionFormula>
    <errorDisplayField>cgcloud__Edits_On_Multiple_Time_Level__c</errorDisplayField>
    <errorMessage>Once enabled, KPI time intervals for months and quarters can&apos;t be disabled.</errorMessage>
</ValidationRule>

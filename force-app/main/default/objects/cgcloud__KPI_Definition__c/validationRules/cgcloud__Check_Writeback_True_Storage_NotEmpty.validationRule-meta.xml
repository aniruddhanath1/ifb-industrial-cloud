<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Check_Writeback_True_Storage_NotEmpty</fullName>
    <active>true</active>
    <errorConditionFormula>AND(cgcloud__Storage_Options_Writeback__c = TRUE, ISBLANK(cgcloud__Storage_Options_Storage_Levels__c))</errorConditionFormula>
    <errorDisplayField>cgcloud__Storage_Options_Storage_Levels__c</errorDisplayField>
    <errorMessage>The Storage Levels can&apos;t be empty if Write Back is enabled.</errorMessage>
</ValidationRule>

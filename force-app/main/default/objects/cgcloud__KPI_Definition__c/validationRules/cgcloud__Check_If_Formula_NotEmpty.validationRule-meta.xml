<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Check_If_Formula_NotEmpty</fullName>
    <active>true</active>
    <description>The Formula can&apos;t be empty.</description>
    <errorConditionFormula>AND( OR(
      $RecordType.DeveloperName = &quot;calculated&quot;, $RecordType.DeveloperName = &quot;editable_calculated&quot;),
      ISBLANK(cgcloud__Formula__c))</errorConditionFormula>
    <errorMessage>The Formula can&apos;t be empty.</errorMessage>
</ValidationRule>

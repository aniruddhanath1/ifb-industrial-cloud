<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LeadSource_Should_Export</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISPICKVAL( Lead_Source_Direct_Lead__c , &quot;Export&quot;)),
NOT(ValidationBypass__c),
NOT(CONTAINS(Country, &quot;India&quot;))
)</errorConditionFormula>
    <errorDisplayField>Lead_Source_Direct_Lead__c</errorDisplayField>
    <errorMessage>Lead Source Should be Export if Country is other than India.</errorMessage>
</ValidationRule>

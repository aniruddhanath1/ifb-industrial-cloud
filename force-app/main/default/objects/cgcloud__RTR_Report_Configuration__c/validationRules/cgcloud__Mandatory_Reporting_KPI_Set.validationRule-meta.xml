<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Mandatory_Reporting_KPI_Set</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR(ISPICKVAL(cgcloud__Usage__c, &apos;reportmeta&apos;), ISPICKVAL(cgcloud__Usage__c, &apos;integrationmeta&apos;)), ISBLANK(cgcloud__Reporting_KPI_Set__c))</errorConditionFormula>
    <errorDisplayField>cgcloud__Reporting_KPI_Set__c</errorDisplayField>
    <errorMessage>Reporting KPI Set cannot be empty, for the selected usage!</errorMessage>
</ValidationRule>

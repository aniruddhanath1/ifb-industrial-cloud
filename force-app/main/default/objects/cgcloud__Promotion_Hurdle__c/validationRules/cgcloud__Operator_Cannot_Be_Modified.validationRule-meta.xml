<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Operator_Cannot_Be_Modified</fullName>
    <active>true</active>
    <description>The operator cannot be changed after hurdle creation.</description>
    <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(cgcloud__Operator__c))</errorConditionFormula>
    <errorDisplayField>cgcloud__Operator__c</errorDisplayField>
    <errorMessage>The Operator can&apos;t be edited after Hurdle creation.</errorMessage>
</ValidationRule>

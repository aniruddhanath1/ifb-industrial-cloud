<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Dashboard_State_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISPICKVAL(cgcloud__Asset_Type__c, &quot;Dashboard&quot;)), NOT(ISBLANK(cgcloud__Dashboard_State__c)))</errorConditionFormula>
    <errorMessage>Date Value can only be provided if Asset Type is &apos;Dashboard&apos;.</errorMessage>
</ValidationRule>

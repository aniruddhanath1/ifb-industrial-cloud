<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_Markup</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
   SBQQ__MarkupRate__c  &lt; 0, 
   SBQQ__MarkupRate__c  &gt; 0.30
)</errorConditionFormula>
    <errorMessage>Markup percentage must be between 0 and 30.</errorMessage>
</ValidationRule>

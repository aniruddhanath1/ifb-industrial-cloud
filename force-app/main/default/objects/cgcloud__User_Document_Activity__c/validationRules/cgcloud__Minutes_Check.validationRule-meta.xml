<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Minutes_Check</fullName>
    <active>true</active>
    <errorConditionFormula>OR(cgcloud__Minutes__c &lt; 0, cgcloud__Minutes__c &gt; 59)</errorConditionFormula>
    <errorMessage>The Minutes must be a number between 0 and 59.</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Validate_Anchor_Product_Level</fullName>
    <active>true</active>
    <description>Anchor Product Level should match the Fund Template Product Level</description>
    <errorConditionFormula>AND(
ISPICKVAL(cgcloud__Fund_Template__r.cgcloud__Anchor_Type__c ,&apos;Customer Product&apos;),
!ISBlank(cgcloud__Anchor_Product__c) ,
(TEXT(cgcloud__Anchor_Product__r.cgcloud__Product_Level__c )&lt;&gt; TEXT( cgcloud__Fund_Template__r.cgcloud__Product_Anchor_Level__c )))</errorConditionFormula>
    <errorMessage>Anchor Product Level should match the Fund Template Product Level</errorMessage>
</ValidationRule>

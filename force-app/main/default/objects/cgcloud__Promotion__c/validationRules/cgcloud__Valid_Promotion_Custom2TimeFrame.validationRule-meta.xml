<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>cgcloud__Valid_Promotion_Custom2TimeFrame</fullName>
    <active>false</active>
    <description>Promotion Custom 2 Time Frame must be within Complete Promotion Time Frame</description>
    <errorConditionFormula>OR(AND(NOT(ISNULL(cgcloud__Custom2_Date_From__c)),cgcloud__Date_From__c &gt; cgcloud__Custom2_Date_From__c),
          AND(NOT(ISNULL(cgcloud__Custom2_Date_Thru__c )),cgcloud__Custom2_Date_Thru__c &gt; cgcloud__Date_Thru__c))</errorConditionFormula>
    <errorMessage>Promotion Custom 2 Time Frame must be within Complete Promotion Time Frame.</errorMessage>
</ValidationRule>
